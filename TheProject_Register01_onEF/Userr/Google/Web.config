<?xml version="1.0"?>
<configuration>
  <system.web>
    <authorization>
      <allow roles="google,userr,admin" />
      <!--Вход разрешен для пользователей в роли admin-->
      <deny users="*" />
      <!--Вход запрещен для всех остальных пользователей-->
    </authorization>
  </system.web>
</configuration>
